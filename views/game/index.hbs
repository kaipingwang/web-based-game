{{# each products }}
<div class="row">
  {{# each this}}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{this.imagePath}}" alt="..." class="img-responsive">
      <div class="caption">
        <h3>{{this.title}}</h3>
        <p class="description">{{this.description}}</p>
        <div class="clearfix"><p class="level pull-left">{{this.level}}</p> <a href="../users/{{this.title}}" class="btn btn-success pull-right" role="button">Play</a></div>
      </div>
    </div>
  </div>
    {{/each}}
  </div>
  {{/each}}
